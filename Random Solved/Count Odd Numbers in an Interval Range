// 1523. Count Odd Numbers in an Interval Range
// https://leetcode.com/problems/count-odd-numbers-in-an-interval-range/description/


class Solution 
{
    public int countOdds(int low, int high) 
    {
        int count = 0;
        for(int i = low; i <= high; i++)
        {
            if((i & 1) == 1) count++;
        }
        return count;
    }
}
