// 191. Number of 1 Bits
// https://leetcode.com/problems/number-of-1-bits/description/

public class Solution 
{
    // you need to treat n as an unsigned value
    public int hammingWeight(int n) 
    {
        int cnt = 0;
        while(n != 0)
        {
            n = n & (n - 1);
            cnt++;
        }
        return cnt;
    }
}
